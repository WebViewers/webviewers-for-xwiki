  #set ($as = $at.getFilename().replaceAll('^.*\.', ''))
#end
#set ($gadgets = $!xwiki.parseGroovyFromPage("Resilience.Code").run($xcontext))
#set ($jioDocName = $doc.getFullName().replaceAll("\.", "/"))
##
